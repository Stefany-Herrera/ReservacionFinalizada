<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="100dp"
        android:layout_height="24dp"
        android:text="  Tipo H"
        app:layout_constraintEnd_toStartOf="@+id/tipo_reservacion"
        app:layout_constraintHorizontal_bias="0.389"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="100dp"
        android:layout_height="24dp"
        android:text="  Precio"
        app:layout_constraintEnd_toStartOf="@+id/precio_reservacion"
        app:layout_constraintHorizontal_bias="0.389"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="100dp"
        android:layout_height="24dp"
        android:text="  Descripcion"
        app:layout_constraintEnd_toStartOf="@+id/descripcion_reservacion"
        app:layout_constraintHorizontal_bias="0.389"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <ImageView
        android:id="@+id/imagen"
        android:layout_width="193dp"
        android:layout_height="121dp"
        android:layout_marginTop="240dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_launcher_background" />

    <TextView
        android:id="@+id/email_reservacion"
        android:layout_width="130dp"
        android:layout_height="24dp"
        android:hint="E-Mail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.775"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/numero_reservacion" />

    <TextView
        android:id="@+id/tipo_reservacion"
        android:layout_width="130dp"
        android:layout_height="24dp"
        android:hint=" Tipo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.775"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/precio_reservacion" />

    <TextView
        android:id="@+id/descripcion_reservacion"
        android:layout_width="130dp"
        android:layout_height="24dp"
        android:hint="Descripcion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.775"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_reservacion" />

    <TextView
        android:id="@+id/numero_reservacion"
        android:layout_width="130dp"
        android:layout_height="24dp"
        android:hint="Numero"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.778"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/apellido_reservacion" />

    <TextView
        android:id="@+id/precio_reservacion"
        android:layout_width="130dp"
        android:layout_height="24dp"
        android:hint="Precio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.775"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/descripcion_reservacion" />

    <TextView
        android:id="@+id/apellido_reservacion"
        android:layout_width="130dp"
        android:layout_height="24dp"
        android:hint="Apellidos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.775"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nombre_reservacion" />

    <TextView
        android:id="@+id/nombre_reservacion"
        android:layout_width="130dp"
        android:layout_height="24dp"
        android:layout_marginTop="24dp"
        android:hint="Nombre"
        android:textColor="#050505"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.772"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="100dp"
        android:layout_height="24dp"
        android:layout_marginTop="28dp"
        android:text=" Nombre"
        app:layout_constraintEnd_toStartOf="@+id/nombre_reservacion"
        app:layout_constraintHorizontal_bias="0.393"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="100dp"
        android:layout_height="24dp"
        android:text=" Apellido"
        app:layout_constraintEnd_toStartOf="@+id/apellido_reservacion"
        app:layout_constraintHorizontal_bias="0.389"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="100dp"
        android:layout_height="24dp"
        android:text="Telefono"
        app:layout_constraintEnd_toStartOf="@+id/numero_reservacion"
        app:layout_constraintHorizontal_bias="0.386"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="100dp"
        android:layout_height="24dp"
        android:text=" Correo"
        app:layout_constraintEnd_toStartOf="@+id/email_reservacion"
        app:layout_constraintHorizontal_bias="0.389"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

</androidx.constraintlayout.widget.ConstraintLayout>